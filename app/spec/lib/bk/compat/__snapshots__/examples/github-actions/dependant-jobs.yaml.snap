---
steps:
- commands:
  - echo ./setup_server.sh
  agents:
    runs-on: ubuntu-latest
  label: ":github: setup"
  key: setup
- commands:
  - echo ./build_server.sh
  depends_on:
  - setup
  agents:
    runs-on: ubuntu-latest
  label: ":github: build"
  key: build
- commands:
  - echo ./test_server.sh
  depends_on:
  - build
  agents:
    runs-on: ubuntu-latest
  label: ":github: test"
  key: test
